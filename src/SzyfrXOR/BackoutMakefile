test: main.o szyfr.o
        cc -o test main.o szyfr.o
test_debug: main_d.o szyfr_d.o
        cc -o test main.o szyfr.o
main.o: main.c
        cc -c main.c
main_d.o: main.c
        cc -c main.c -DDEBUG
szyfr_d.o: szyfr.c
        cc -c szyfr.c -DDEBUG
szyfr.o: szyfr.c
        cc -c szyfr.c

clean: main.o szyfr.o
        rm main.o szyfr.o test
